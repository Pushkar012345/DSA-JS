/**
 * @param {string} s
 * @return {boolean}
 */
class Solution {
    isPalindrome(s) {
        // code here
        
        let len=s.length;
        
        for(let i=0;i<Math.floor(len/2);i++)
        {
            if(s[i]!=s[len-i-1])
            {
                return false;
            }
        }
        
        return true;
    }
}
