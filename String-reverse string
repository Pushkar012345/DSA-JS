// User function Template for javascript

/**
 * @param {string} s
 * @return {string}
 */

class Solution {

    reverseString(s) {
        //logic:-
        //S length do and back coem n print
        //
        
        let n=s.length;
        
        let result="";
        
        for(let i=n-1;i>=0;i--)
        {
            result+=s[i];
        }
        
        return result;
    }
}
