/**
 * @param {string} s
 * @returns {boolean}
 */

class Solution {
    isBinary(s) {
        // code here
        let n=s.length;
        
        for(let i=0;i<n;i++)
        {
            if(s[i]!='0' && s[i]!=1)
            {
                return false;
            }
        }
        
        return true;
    }
}
